{
  "expo": {
    "name": "Birdr",
    "slug": "birdr-mobile",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "birdr",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "pro.birdr.app",
      "buildNumber": "18",
      "icon": "./assets/icon.png",
      "associatedDomains": ["applinks:birdr.pro"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "pro.birdr.app",
      "versionCode": 18,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "birdr.pro",
              "pathPrefix": "/join/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "plugins": [
      "expo-asset",
      [
        "expo-image-picker",
        {
          "photosPermission": "Birdr needs access to your photo library to set your profile picture.",
          "cameraPermission": "Birdr needs access to your camera to take a profile picture."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.56451813101-kkjv7e0agsujmmtuuq5gs6mdkumdmp71"
        }
      ]
    ]
  }
}