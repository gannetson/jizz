[run]
source = jizz,compare,media
omit =
    */migrations/*
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */.virtualenvs/*
    manage.py
    */scrapers/*
    *scraper*.py
    */management/commands/*scrape*.py
    *standardize_copyright*
    *ai_service*
    */media/utils*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if TYPE_CHECKING:
    if __name__ == .__main__.:

[html]
directory = htmlcov
